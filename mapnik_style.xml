<?xml version="1.0" encoding="utf-8"?>
<Map background-color="#f2efe9" srs="+proj=latlong +datum=WGS84">
	<FontSet name="book-fonts">
		<Font face-name="DejaVu Sans Book" />
	</FontSet>
	<Style name="buildings">
		<Rule>
			<Filter>[building] &lt;&gt;''</Filter>
			<LineSymbolizer stroke="#808080" stroke-width="2" stroke-linejoin="round"
				stroke-linecap="round" />
            <PolygonSymbolizer />

		</Rule>
	</Style>
	<Layer name="buildings" status="on" srs="+proj=latlong +datum=WGS84">
		<StyleName>buildings</StyleName>
		<Datasource>
			<Parameter name="type">osm</Parameter>
			<Parameter name="file">map.osm</Parameter>
		</Datasource>
	</Layer>
</Map>